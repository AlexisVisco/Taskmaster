{
  "programs": [
    {
      "name": "ls",
      "autorestart": "never",
      "autostart": true,
      "cmd": "ls -aRg /",
      "env": {
        "ANSWER": 42,
        "STARTED_BY": "taskmaster"
      },
      "exitcodes": [
        0,
        1
      ],
      "numprocs": 3,
      "startretries": 3,
      "starttime": 15,
      "stderr": "/tmp/nginx.stderr",
      "stdout": "/tmp/nginx.stdout",
      "stopsignal": "TERM",
      "stoptime": 10,
      "umask": 18,
      "workingdir": "/tmp"
    },
    {
      "name": "cat",
      "autorestart": "never",
      "autostart": true,
      "cmd": "cat *",
      "exitcodes": [
        0,
        2
      ],
      "numprocs": 2,
      "startretries": 3,
      "starttime": 5,
      "stopsignal": "TERM",
      "stoptime": 10,
      "umask": 18,
      "workingdir": "/tmp"
    }
  ]
}